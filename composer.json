{
    "name": "beartropy/tables",
    "version": "2.7.1",
    "description": "This is yet another laravel livewire table and come as is. You can filter, you can sort, you can bulk, toggle columns, the basics.",
    "license": "MIT",
    "keywords": [
        "livewire table",
        "table",
        "blade components",
        "laravel components",
        "livewire components"
    ],
    "require": {
        "php": "^8.2",
        "beartropy/ui": "*"
    },
    "autoload": {
        "psr-4": {
            "Beartropy\\Tables\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "livewire/livewire": "^3.7"
    },
    "authors": [
        {
            "name": "Beartropy"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Beartropy\\Tables\\YATProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}